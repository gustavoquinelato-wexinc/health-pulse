# Frontend App Environment Configuration
# This file contains environment variables specific to the React frontend

# API Endpoints
VITE_BACKEND_SERVICE_URL=http://localhost:3002
VITE_ETL_SERVICE_URL=http://localhost:8000

# Development Settings
VITE_NODE_ENV=development
VITE_APP_NAME=Pulse Platform
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_DEBUG_LOGS=true
VITE_ENABLE_THEME_SWITCHING=true

# Portal Configuration
VITE_PORTAL_MODE=iframe
VITE_DEFAULT_THEME=light
VITE_DEFAULT_COLOR_SCHEMA=default
